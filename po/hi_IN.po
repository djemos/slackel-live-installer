msgid "Language"
msgstr "भाषा"

#: src/sli.ui:899
msgid "Select a default system locale."
msgstr "सिस्टम हेतु डिफ़ॉल्ट स्थानिकी चुनें।"

#: src/sli.ui:923
msgid "keyboard"
msgstr "कुंजीपटल"

